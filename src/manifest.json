{
  "manifest_version": 2,
  "name": "Github Event Filter",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "main.js",
    "activity/index.js",
    "commit/index.js"
 ]
}